import java.io.BufferedReader;
import java.io.FileInputStream;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class Main2979 {
	
	public static int a;
	public static int b;
	public static int c;
	public static int pay;
	public static Car[] cars;
	
	public static void main(String[] args) throws Exception{
		System.setIn(new FileInputStream("input2979.txt"));
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		StringTokenizer st = new StringTokenizer(br.readLine());
		a = Integer.parseInt(st.nextToken());
		b = Integer.parseInt(st.nextToken());
		c = Integer.parseInt(st.nextToken());
		cars = new Car[3];
		int arrive=0, leave=0;
		for(int i=0; i<3; i++) {
			st = new StringTokenizer(br.readLine());
			arrive = Integer.parseInt(st.nextToken());
			leave = Integer.parseInt(st.nextToken());
			cars[i] = new Car(arrive,leave);
		}
		
		int count = 0;
		int carCount = 0;
		while(true) {
			count++;
			for(int i=0; i<3; i++) {
				if(count==cars[i].arrive) carCount++;
				else if(count==cars[i].leave) carCount--; 
			}
		}
		
		br.close();
	}
	
	public static class Car {
		int arrive;
		int leave;
		public Car(int arrive, int leave) {
			this.arrive = arrive;
			this.leave = leave;
		}
	}

}
